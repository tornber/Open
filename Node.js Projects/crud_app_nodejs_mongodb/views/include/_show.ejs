<% users.forEach((user,ind) => { %> 
<tr>
    <td><%= ind + 1 %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.gender %></td>
    <td><%= user.status %></td>
    <td>
        <a href="/update-user?id=<%= user._id %> " class="btn border-shadow update">
            <span class="text-gradient"><i class="fa-solid fa-pen-to-square"></i></span>
        </a>
        <a href="/delete-user/<%=user._id%>" class="btn border-shadow delete" >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% }) %> 